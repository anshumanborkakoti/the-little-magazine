{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/src/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://res.cloudinary.com",
          "https://fonts.google.com",
          "https://fonts.gstatic.com",
          "https://s3.ap-south-1.amazonaws.com/issues.the-little-journal.com"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "tljneapi",
      "urls": [
        "https://api.the-little-journal.com/tljneapi/categories",
        "https://api.the-little-journal.com/tljneapi/issues",
        "https://api.the-little-journal.com/tljneapi/posts"
      ],
      "version": 2,
      "cacheConfig": {
        "maxAge": "30d",
        "timeout": "15s",
        "maxSize": 100,
        "strategy": "performance"
      }
    },
    {
      "name": "comments",
      "urls": ["https://api.the-little-journal.com/tljneapi/comments"],
      "version": 2,
      "cacheConfig": {
        "maxAge": "1h",
        "timeout": "15s",
        "maxSize": 100,
        "strategy": "freshness"
      }
    }
  ]
}
